<h1>Cats</h1>
<br>
<% @cats.each do | cat | %>
		<tr>
		<td><%= cat.name %></td>
		<br>
		</tr>
	<% end %>
<br>

<h1>Users</h1>
<br>
<% @users.each do | user | %>
		<tr>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= user.age %></td>
		<br>
		</tr>
	<% end %>
<br>

<h1>Todos</h1>
<br>
<% @todos.each do | todo | %>
		<tr>
		<td><%= todo.tasks %></td>
		<td><%= todo.finished %></td>
		<br>
		</tr>
	<% end %>
<br>